		function change_table1() {
			var x = document.getElementById("sltapc1").value;
			    
			if (x === 'Militant') {
				document.getElementById("mil2").style.display = 'none',
				document.getElementById("td1").innerHTML = nileImg,
				document.getElementById("td2").innerHTML = nileName,
				document.getElementById("td3").innerHTML = "S0",
				document.getElementById("td4").innerHTML = f,
				document.getElementById("td5").innerHTML = "1 , 2",
				document.getElementById("td6").innerHTML = si;
			}
			if (x === 'Forsaken') {
				document.getElementById("mil2").style.display = 'block',
				document.getElementById("td1").innerHTML = forsakenImg,
				document.getElementById("td2").innerHTML = forsakenName,
				document.getElementById("td3").innerHTML = "S0",
				document.getElementById("td4").innerHTML = a,
				document.getElementById("td5").innerHTML = 1,
				document.getElementById("td6").innerHTML = si + ", " + wd;
			}
			if (x === 'Destroya') {
				document.getElementById("mil2").style.display = 'block',
				document.getElementById("td1").innerHTML = destroyaImg,
				document.getElementById("td2").innerHTML = destroyaName,
				document.getElementById("td3").innerHTML = "S0",
				document.getElementById("td4").innerHTML = a,
				document.getElementById("td5").innerHTML = 1,
				document.getElementById("td6").innerHTML = "-";
			}
            if (x === 'Courageous') {
                document.getElementById("mil2").style.display = 'block',
                document.getElementById("td1").innerHTML = courageousImg,
                document.getElementById("td2").innerHTML = courageousName,
				document.getElementById("td3").innerHTML = "S1",
				document.getElementById("td4").innerHTML = f,
				document.getElementById("td5").innerHTML = 1,
				document.getElementById("td6").innerHTML = "-";
            }
            if (x === 'Jax') {
            	document.getElementById("mil2").style.display = 'block',
                document.getElementById("td1").innerHTML = jaxImg,
                document.getElementById("td2").innerHTML = jaxName,
				document.getElementById("td3").innerHTML = "S1",
				document.getElementById("td4").innerHTML = v,
				document.getElementById("td5").innerHTML = 1,
				document.getElementById("td6").innerHTML = md + ", " + fd + ", " + d;
            }
            if (x === 'Falcon') {
            	document.getElementById("mil2").style.display = 'block',
                document.getElementById("td1").innerHTML = falconImg,
                document.getElementById("td2").innerHTML = falconName,
				document.getElementById("td3").innerHTML = "S1",
				document.getElementById("td4").innerHTML = v,
