		const cAll = document.querySelector('.cAll');
		const cFighters = document.querySelector("option.cFighters");
		const cVehicles = document.querySelector('.cVehicles');
		const cShooters = document.querySelector('.cShooters');
		const all = document.querySelector('#all');
		const fighters = document.querySelector('#fighters');
		const vehicles = document.querySelector('#vehicles');
		const shooters = document.querySelector('#shooters');

		all.addEventListener('click', () => {
			cFighters.classList.remove('hidden');
			cVehicles.classList.remove('hidden');
			cShooters.classList.remove('hidden');
		})

		fighters.addEventListener('click', function() {
			cFighters.classList.remove('hidden');
			cVehicles.classList.add('hidden');
			cShooters.classList.add('hidden');
		})

		vehicles.addEventListener('click', () => {
			cFighters.classList.add('hidden');
			cVehicles.classList.remove('hidden');
			cShooters.classList.add('hidden');
		})

		shooters.addEventListener('click', () => {
			cFighters.classList.add('hidden');
			cVehicles.classList.add('hidden');
			cShooters.classList.remove('hidden');
		})

		 
		 
	</script>
</body>
</html>
